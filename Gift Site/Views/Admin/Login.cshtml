<h2>Admin Login</h2>
<form asp-action="Login" method="post">
    <div>
        <label>Email</label>
        <input type="email" name="email" />
    </div>
    <div>
        <label>Password</label>
        <input type="password" name="password" />
    </div>
    <button type="submit">Login</button>
</form>
<p>@ViewBag.Error</p>
