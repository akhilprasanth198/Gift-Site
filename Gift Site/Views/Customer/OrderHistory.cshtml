@model IEnumerable<Gift_Site.Models.Order>

<h2>Order History</h2>
<table>
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Total Rate</th>
            <th>Status</th>
            <th>Order Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.TotalRate</td>
                <td>@order.Status</td>
                <td>@order.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>
